{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:ci": "prettier --check \"src/**/*.ts\" && eslint \"{src,tests}/**/*.ts\"",
    "migration": "knex migrate:latest",
    "seed": "knex seed:run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": "bash -c tsc --noEmit",
    "src/**/*.+(js|json|ts)": "npm run lint",
    "src/**/*.+(js|json|css|md|ts)": "npm run format"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.2.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/platform-socket.io": "^8.0.6",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.0.6",
    "apollo-cache-control": "^0.14.0",
    "apollo-server": "^2.25.2",
    "apollo-server-core": "^2.24.0",
    "apollo-server-errors": "^3.0.1",
    "apollo-server-express": "^2.25.2",
    "apollo-upload-server": "^7.1.0",
    "aws-sdk": "^2.968.0",
    "cloudinary": "^1.26.3",
    "date-fns": "^2.23.0",
    "date-fns-timezone": "^0.1.4",
    "date-fns-tz": "^1.1.6",
    "firebase": "^8.9.1",
    "firebase-admin": "^9.11.0",
    "graphql": "^15.5.1",
    "graphql-query-complexity": "^0.9.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-tools": "^8.1.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^12.0.0",
    "graphql-upload-minimal": "^1.3.0",
    "ioredis": "^4.27.7",
    "iso-639-1": "^2.1.10",
    "knex": "^2.1.0",
    "lodash": "^4.17.21",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.8",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.0.6",
    "@types/bull": "^3.15.3",
    "@types/express": "^4.17.7",
    "@types/ffmpeg": "^1.0.4",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.6",
    "@types/ioredis": "^4.26.7",
    "@types/jest": "^27.0.1",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": "^12.1.7",
    "prettier": "^2.3.2",
    "prettier-eslint-cli": "^5.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}